{
    "name": "mi-aplicacion", // Nombre de tu proyecto en Vercel
    "version": 2, // Versión de la configuración de Vercel (siempre usa 2)
    "builds": [
      { 
        "src": "./src/index.js", 
        "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}
  